{
    "name": "tightenco/lambo",
    "description": "Super-powered 'laravel new' with Laravel and Valet.",
    "keywords": ["laravel", "zonda", "wwdhhd", "lambo"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tightenco/lambo/issues",
        "source": "https://github.com/tightenco/lambo"
    },
    "authors": [
        {
            "name": "Matt Stauffer",
            "email": "matt@mattstauffer.co"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "5.6.*",
        "laravel-zero/framework": "5.6.*",
        "vlucas/phpdotenv": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["lambo"]
}
